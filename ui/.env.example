# OpenObserve Browser RUM Configuration
# Set to 'true' to enable frontend observability (RUM + Logs)
VITE_OPENOBSERVE_ENABLED=false

# OpenObserve endpoint
VITE_OPENOBSERVE_ENDPOINT=

# OpenObserve client token (RUM token from OpenObserve UI)
VITE_OPENOBSERVE_CLIENT_TOKEN=

# OpenObserve organization identifier
VITE_OPENOBSERVE_ORG=default

# Application metadata
VITE_OPENOBSERVE_APP_ID=hornet2-ui
VITE_OPENOBSERVE_SERVICE=hornet2-ui

# Environment (development, staging, production)
VITE_OPENOBSERVE_ENV=development

# API Version
VITE_OPENOBSERVE_API_VERSION=v1

# Privacy level: 'allow', 'mask-user-input', or 'mask'
VITE_OPENOBSERVE_PRIVACY_LEVEL=mask-user-input
